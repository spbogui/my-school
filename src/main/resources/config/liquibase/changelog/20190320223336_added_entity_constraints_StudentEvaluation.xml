<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity StudentEvaluation.
    -->
    <changeSet id="20190320223336-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="actor_id"
                                 baseTableName="student_evaluation"
                                 constraintName="fk_student_evaluation_actor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actor"/>

        <addForeignKeyConstraint baseColumnNames="evaluation_id"
                                 baseTableName="student_evaluation"
                                 constraintName="fk_student_evaluation_evaluation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="evaluation"/>

        <addForeignKeyConstraint baseColumnNames="evaluation_mode_id"
                                 baseTableName="student_evaluation"
                                 constraintName="fk_student_evaluation_evaluation_mode_id"
                                 referencedColumnNames="id"
                                 referencedTableName="evaluation_mode"/>

        <addForeignKeyConstraint baseColumnNames="subject_id"
                                 baseTableName="student_evaluation"
                                 constraintName="fk_student_evaluation_subject_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subject"/>

    </changeSet>
</databaseChangeLog>
