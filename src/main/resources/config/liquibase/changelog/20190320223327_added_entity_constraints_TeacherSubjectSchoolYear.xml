<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity TeacherSubjectSchoolYear.
    -->
    <changeSet id="20190320223327-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="actor_id"
                                 baseTableName="teacher_subject_school_year"
                                 constraintName="fk_teacher_subject_school_year_actor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actor"/>

        <addForeignKeyConstraint baseColumnNames="teacher_id"
                                 baseTableName="teacher_subject_school_year"
                                 constraintName="fk_teacher_subject_school_year_teacher_id"
                                 referencedColumnNames="id"
                                 referencedTableName="teacher"/>

        <addForeignKeyConstraint baseColumnNames="school_school_year_id"
                                 baseTableName="teacher_subject_school_year"
                                 constraintName="fk_teacher_subject_school_year_school_school_year_id"
                                 referencedColumnNames="id"
                                 referencedTableName="school_school_year"/>

    </changeSet>
</databaseChangeLog>
