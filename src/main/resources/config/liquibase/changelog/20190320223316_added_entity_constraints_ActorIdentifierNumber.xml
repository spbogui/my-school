<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity ActorIdentifierNumber.
    -->
    <changeSet id="20190320223316-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="identifier_type_id"
                                 baseTableName="actor_identifier_number"
                                 constraintName="fk_actor_identifier_number_identifier_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="identifier_type"/>

        <addForeignKeyConstraint baseColumnNames="actor_id"
                                 baseTableName="actor_identifier_number"
                                 constraintName="fk_actor_identifier_number_actor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actor"/>

    </changeSet>
</databaseChangeLog>
