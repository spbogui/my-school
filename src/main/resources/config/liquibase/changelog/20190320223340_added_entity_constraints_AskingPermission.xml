    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity AskingPermission.
    -->
    <changeSet id="20190320223340-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="school_school_year_id"
