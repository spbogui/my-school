    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity SchoolSchoolYear.
    -->
    <changeSet id="20190320223256-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="school_name_id"
                                 baseTableName="school_school_year"
