<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Evaluation.
    -->
    <changeSet id="20190320223335-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="evaluation_type_id"
                                 baseTableName="evaluation"
                                 constraintName="fk_evaluation_evaluation_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="evaluation_type"/>

        <addForeignKeyConstraint baseColumnNames="school_school_year_id"
                                 baseTableName="evaluation"
                                 constraintName="fk_evaluation_school_school_year_id"
                                 referencedColumnNames="id"
                                 referencedTableName="school_school_year"/>

        <addForeignKeyConstraint baseColumnNames="period_id"
                                 baseTableName="evaluation"
                                 constraintName="fk_evaluation_period_id"
                                 referencedColumnNames="id"
                                 referencedTableName="period"/>

        <addForeignKeyConstraint baseColumnNames="evaluation_id"
                                 baseTableName="evaluation_class_room"
                                 constraintName="fk_evaluation_class_room_evaluation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="evaluation"/>
        <addForeignKeyConstraint baseColumnNames="class_room_id"
                                 baseTableName="evaluation_class_room"
                                 constraintName="fk_evaluation_class_room_class_room_id"
                                 referencedColumnNames="id"
                                 referencedTableName="class_room"/>
        
    </changeSet>
</databaseChangeLog>
